{
  "name": "react-leaflet-google-layer",
  "version": "1.0.8",
  "description": "React leaflet wrapper of leaflet.gridlayer.googlemutant",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aviklai/react-leaflet-google-mutant.git"
  },
  "keywords": [
    "React",
    "Leaflet",
    "google",
    "googlemutant",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aviklai/react-leaflet-google-mutant/issues"
  },
  "homepage": "https://github.com/aviklai/react-leaflet-google-mutant#readme",
  "peerDependencies": {
    "leaflet": "^1.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-leaflet": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",    
    "@types/react": "^16.8.7",
    "@types/react-leaflet": "^2.2.0",
    "jest": "^24.3.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "leaflet": "^1.3.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-leaflet": "^2.0.0"
  },
  "dependencies": {
    "leaflet.gridlayer.googlemutant": "^0.8.0",
    "@types/leaflet": "^1.4.1",
    "@types/leaflet.gridlayer.googlemutant": "^0.4.4"   
  }
}
