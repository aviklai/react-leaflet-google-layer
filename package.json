{
  "name": "react-leaflet-google-layer",
  "version": "3.0.0",
  "description": "React leaflet wrapper of leaflet.gridlayer.googlemutant",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc && npm run minify",
    "minify": "uglifyjs --compress --mangle --output lib/index.min.js -- lib/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "cover": "jest --config jestconfig.json --coverage"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aviklai/react-leaflet-google-layer.git"
  },
  "keywords": [
    "React",
    "Leaflet",
    "google",
    "googlemutant",
    "typescript",
    "react-leaflet-v3",
    "react-leaflet-v4"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aviklai/react-leaflet-google-layer/issues"
  },
  "homepage": "https://github.com/aviklai/react-leaflet-google-layer#readme",
  "peerDependencies": {
    "leaflet": ">=1.5.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-leaflet": ">=3.0.0"
  },
  "devDependencies": {
    "@googlemaps/jest-mocks": "^0.1.1",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.38",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-test-renderer": "^16.9.3",
    "coveralls": "^3.1.0",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.1.0",
    "leaflet": ">=1.5.0",
    "prettier": "^1.16.4",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-leaflet": ">=3.0.0",
    "react-test-renderer": ">=16.8.0",
    "ts-jest": "^28.0.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5",
    "uglify-js": "^3.5.15"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.8.0",
    "@types/leaflet": "^1.5.0",
    "@types/leaflet.gridlayer.googlemutant": "^0.4.9",
    "leaflet.gridlayer.googlemutant": "^0.14.1"
  }
}
